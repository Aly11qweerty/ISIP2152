        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TourApp4"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="13*"/>
            <ColumnDefinition Width="5*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height=""/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="Resources\tourApp4.jpg" HorizontalAlignment="Left"></Image>
        <TextBlock Text="Tours" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="361,18,140.146,17"></TextBlock>
        <Grid Background="AntiqueWhite" Panel.ZIndex="-2" Grid.ColumnSpan="2"></Grid>
